	"Rafstermancppbubbleshort":{
		"prefix": "cppbubbleshort",
		"body": [
			"void bubbleSort(int arr[]){",
			"    for(int i = 0; i < (sizeof(arr)/sizeof(arr[0])) - 1; i++){",
			"        for (j = 0; j < (sizeof(arr)/sizeof(arr[0])) - i - 1; j++){",
            "            if (arr[j] > arr[j + 1]){",
			"                swap(arr[j], arr[j + 1]);",
			"            }",
			"        }",
			"    }",
			"}"
		],
		"description": "Metodo De Ordenamiento Bubblesort",
		"scope": "cpp,ino"
